" vim:ft=vim

Execute(test-silent):
  Log '# FloatermNew --silent ls'
    FloatermNew --silent ls
    sleep 100m
    AssertEqual 0, len(floaterm#buflist#gather())

  Log '# FloatermNew! --silent ls'
    FloatermNew! --silent
    AssertEqual 1, len(floaterm#buflist#gather())

  FloatermKill!
  stopinsert
